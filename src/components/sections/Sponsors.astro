---
import { Image } from "astro:assets";
import s1 from "../../assets/pics/portalinmobiliario.png";
import Section from "./Section.astro";

const sponsors = [
  { logo: s1, alt: "Logo Portal Inmobiliario" },
];
---

<Section id="sponsors">
  <div class="flex flex-wrap justify-between gap-7 mt-[60px] *:*:max-w-120px">
    {
      sponsors.map((sponsor) => {
        return (
          <div>
            <Image src={sponsor.logo} alt={sponsor.alt} loading="eager" />
          </div>
        );
      })
    }
  </div>
</Section>
